set autoindent
set background=dark
set backspace=indent,eol,start
set backup
set cmdheight=2
set cursorline
set diffexpr=MyDiff()
set display=truncate
set encoding=utf8
set expandtab
set foldcolumn=1
set foldenable
set foldmethod=indent
set foldlevelstart=10
set foldnestmax=10
set guifont=Fira_Code:h11:cANSI:qDRAFT
set guioptions=egmrL
set guioptions-=T
set history=500
set hlsearch
set incsearch
set lines=40 columns=120
set nu
set ruler
set runtimepath=~/vimfiles,C:\\Program\ Files\ (x86)\\Vim/vimfiles,C:\\Program\ Files\ (x86)\\Vim\\vim81,C:\\Program\ Files\ (x86)\\Vim\\vim81\\pack\\dist\\opt\\matchit,C:\\Program\ Files\ (x86)\\Vim/vimfiles/after,~/vimfiles/after
set scrolloff=5
set selection=exclusive
set selectmode=mouse,key
set showmatch
set tabstop=4
set expandtab
set shiftwidth=4
set softtabstop=4
set undofile
set whichwrap=b,s,<,>,[,]
syntax enable
filetype indent on
let python_highlight_all = 1

colorscheme base16-tomorrow-night

nnoremap <space> za
